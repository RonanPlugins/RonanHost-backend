{
  "name": "RonanHost-Backend",
  "version": "1.3.2",
  "type": "module",
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "npm run build && node -r tsconfig-paths/register dist/src/app.js",
    "build": "tsc --build tsconfig.json",
    "dev": "node -r tsconfig-paths/register dist/src/app.js"
  },
  "dependencies": {
    "@avionrx/pterodactyl-js": "^2.1.2",
    "@types/axios": "^0.14.0",
    "@types/dotenv": "^8.2.0",
    "@types/stripe": "^8.0.417",
    "@types/uuid": "^9.0.1",
    "bcrypt": "^5.1.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-mysql-session": "^3.0.0",
    "express-session": "^1.17.3",
    "js-yaml": "^4.1.0",
    "mysql": "^2.18.1",
    "passport": "^0.6.0",
    "passport-local": "^1.0.0",
    "powerbi-client": "^2.22.2",
    "stripe": "^12.0.0",
    "tsconfig-paths": "^4.2.0",
    "typeorm": "^0.3.14",
    "uuid": "^9.0.0"
  },
  "imports": {
    "#User": [
      "./models/User"
    ],
    "#UUID": [
      "./util/functions/uuid"
    ],
    "#repositories/*": [
      "./repositories/*"
    ],
    "#functions/*": [
      "./util/functions/*"
    ],
    "#util/*": [
      "./util/*"
    ],
    "#models/*": [
      "./models/*"
    ],
    "#baseRepository": [
      "./repositories/Base/BaseRepository"
    ],
    "#baseService": [
      "./services/Base/BaseService"
    ],
    "#services/*": [
      "./services/*"
    ],
    "#data/*": [
      "./util/data/*"
    ],
    "#decorators/*": [
      "./util/decorators/*"
    ],
    "#errors/*": [
      "./Error/*"
    ],
    "#NotFoundError": [
      "./Error/NotFoundError"
    ],
    "#DuplicateError": [
      "./Error/DuplicateError"
    ],
    "#MissingValuesError": [
      "./Error/MissingValuesError"
    ],
    "#config": [
      "./config"
    ],
    "#database": [
      "./util/data/database"
    ],
    "#crypto": [
      "./util/security/crypto"
    ],
    "#stripe-api-client": [
      "./util/external/clients/stripe-api-client"
    ]
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/mysql": "^2.15.21",
    "@types/node": "^18.15.11",
    "typescript": "^5.0.4"
  }
}
