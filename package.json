{
  "name": "RonanHost-Backend",
  "version": "1.2.5",
  "type": "module",
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "node app.js",
    "build": "tsc --build tsconfig.json",
    "dev": "node app.js"
  },
  "dependencies": {
    "@avionrx/pterodactyl-js": "^2.1.2",
    "@types/axios": "^0.14.0",
    "@types/dotenv": "^8.2.0",
    "@types/stripe": "^8.0.417",
    "@types/uuid": "^9.0.1",
    "bcrypt": "^5.1.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-mysql-session": "^3.0.0",
    "express-session": "^1.17.3",
    "js-yaml": "^4.1.0",
    "mysql": "^2.18.1",
    "powerbi-client": "^2.22.2",
    "pterodactyl.js": "^2.1.1",
    "stripe": "^12.0.0",
    "typeorm": "^0.3.14",
    "uuid": "^9.0.0"
  },
  "imports": {
    "#User": [
      "./src/models/User.js"
    ],
    "#repositories/*": [
      "./src/repositories/*.js"
    ],
    "#decorators/*": [
      "./src/util/decorators/*.js"
    ],
    "#services/*": [
      "./src/services/*.js"
    ],
    "#models/*": [
      "./src/models/*.js"
    ],
    "#data/*": [
      "./src/util/data/*.js"
    ],
    "#errors/*": [
      "./src/Error/*.js"
    ],
    "#NotFoundError": [
      "./src/Error/NotFoundError.js"
    ],
    "#DuplicateError": [
      "./src/Error/DuplicateError.js"
    ],
    "#MissingValuesError": [
      "./src/Error/MissingValuesError.js"
    ],
    "#config": [
      "./src/config.js"
    ],
    "#database": [
      "./src/util/data/database.js"
    ],
    "#crypto": [
      "./src/util/security/crypto.js"
    ],
    "#UUID": [
      "./src/types/UUID.js"
    ],
    "#stripe-api-client": [
      "./src/util/external/clients/stripe-api-client.js"
    ],
    "#baseRepository": [
      "./src/repositories/Base/BaseRepository.js"
    ],
    "#baseService": [
      "./src/services/Base/BaseService.js"
    ]
  },
  "devDependencies": {
    "@types/mysql": "^2.15.21",
    "@types/node": "^18.15.11"
  }
}
