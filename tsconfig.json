{
  "compilerOptions": {
    "target": "es2020",
    "moduleResolution": "node",
    "module": "es2020",
    "declaration": false,
    "esModuleInterop": true,
    "sourceMap": false,
    "paths": {
      "#User": ["./models/User.js"],
      "#repositories/*": [
        "./repositories/*.js",
        "./repositories/*.ts"
      ],
      "#services/*": [
        "./services/*.js",
        "./services/*.ts"
      ],
      "#NotFoundError": ["./Error/NotFoundError.js"],
      "#DuplicateError": ["./Error/DuplicateError.js"],
      "#MissingValuesError": ["./Error/MissingValuesError.js"],
      "#config": ["./config.js"],
      "#database": ["./lib/functions/database/db.js"],
      "#crypto": ["./util/security/"]
    }
  },
  "include": [
    "src",
    "app.ts"
  ]
}