{
  "compilerOptions": {
    "experimentalDecorators": true,
    "target": "es2020",
    "moduleResolution": "node",
    "module": "es2020",
    "declaration": false,
    "esModuleInterop": true,
    "strictPropertyInitialization": false,
    "skipLibCheck": true,
    "sourceMap": false,
    "paths": {
      "#User": ["./src/models/User.js"],
      "#UUID": ["./src/types/UUID.js"],
      "#repositories/*": [
        "./src/repositories/*.js",
        "./src/repositories/*.ts",
      ],
      "#models/*": [
        "./src/models/*.js",
        "./src/models/*.ts"
      ],
      "#baseRepository": ["./src/repositories/Base/BaseRepository.js"],
      "#baseService": ["./src/services/Base/BaseService.js"],
      "#services/*": [
        "./src/services/*.js",
        "./src/services/*.ts"
      ],
      "#data/*": [
        "./src/util/data/*.js",
        "./src/util/data/*.ts"
      ],
      "#decorators/*": [
        "./src/util/decorators/*.ts",
        "./src/util/decorators/*.js"
      ],
      "#errors/*": [
        "./src/Error/*.ts",
        "./src/Error/*.js"
      ],
      "#NotFoundError": ["./src/Error/NotFoundError.js"],
      "#DuplicateError": ["./src/Error/DuplicateError.js"],
      "#MissingValuesError": ["./src/Error/MissingValuesError.js"],
      "#config": ["./src/config.js"],
      "#database": ["./src/util/data/database.js"],
      "#crypto": ["./src/util/security/crypto.js"],
      "#stripe-api-client": ["./src/util/external/clients/stripe-api-client.js"]
    }
  },
  "include": [
    "src",
    "app.ts"
  ]
}