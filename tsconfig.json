{
  "compilerOptions": {
    "target": "es2020",
    "moduleResolution": "node",
    "module": "es2020",
    "declaration": false,
    "esModuleInterop": true,
    "sourceMap": false,
    "paths": {
      "#User": ["./src/models/User.js"],
      "#repositories/*": [
        "./src/repositories/*.js",
        "./src/repositories/*.ts"
      ],
      "#services/*": [
        "./src/services/*.js",
        "./src/services/*.ts"
      ],
      "#NotFoundError": ["./src/Error/NotFoundError.js"],
      "#DuplicateError": ["./src/Error/DuplicateError.js"],
      "#MissingValuesError": ["./src/Error/MissingValuesError.js"],
      "#config": ["./src/config.js"],
      "#database": ["./src/lib/functions/database/db.js"],
      "#crypto": ["./src/util/security/"]
    }
  },
  "include": [
    "src",
    "app.ts"
  ]
}