{
  "compilerOptions": {
    "target": "es2020",
    "moduleResolution": "node",
    "module": "es2020",
    "outDir": "dist",
    "declaration": false,
    "esModuleInterop": true,
    "sourceMap": false,
    "paths": {
      "#User": ["./models/User.js"],
      "#repositories/*": [
        "./models/repositories/*.js",
        "./models/repositories/*.ts"
      ],
      "#services/*": [
        "./models/services/*.js",
        "./models/services/*.ts"
      ],
      "#NotFoundError": ["./models/Error/NotFoundError.js"],
      "#DuplicateError": ["./models/Error/DuplicateError.js"],
      "#MissingValuesError": ["./models/Error/MissingValuesError.js"],
      "#config": ["./config.js"],
      "#database": ["./lib/functions/database/db.js"]
    }
  },
  "include": [
    "src"
  ]
}